/* pages/mine/mine.wxss */
.totle-list{
  width: 100%;
  background: #eee;
  height: 100%;
}
.setup-list {
  background-color: #fff; 
}
.setup-list navigator {
  display: flex;
  justify-content:space-between;
  padding: 30rpx 30rpx;
  border-bottom: 1rpx solid #e8e8e8;
}
.setup-list navigator text{
  font-size: 36rpx;
  color:#6b6b6b;
  margin-right:auto;
}
.setup-list navigator .mineIcon{
   height: 65rpx;
   width: 65rpx;
}
.setup-list navigator image{
  height: 35rpx;
  width: 35rpx;
  padding-right: 10px;
}
.mineUser{
  width: 100%;
  height:170px;
  margin-bottom: 5%;
  background-color: #fff;
}
.mineUserTop{
width:100%;
height:10px;
}
.mineUserTop image{
  width:30px;
  height: 30px;
  padding: 10px;
  float: right;
}
.mineUserImage{
  width:30%;
  padding-top: 40px;
  padding-left: 20px;
  height:80px;
  float: left;
}
.mineUserImage image{
 width: 100%;
 height: 100px;
 border-radius: 100%;
}
.mineUserText{
  width:30%;
  padding-top: 20%;
  padding-left: 10px;
  float: left;
}
.mineUserText text{
  font-size: 22px;
}